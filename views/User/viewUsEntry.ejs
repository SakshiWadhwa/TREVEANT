<%- include header %> 
<style>
	.tab
	{
		padding-top: 120px;
	}
</style>

	<div class="table-responsive container tab">
		<h1 class="text-center"><%= data[0].user_firstname %></h1>
		<table class="table table-striped table-hover table-bordered table-condensed">

			<thead>
				<tr>
					<th>User_Id</th>
					<th>Firstname</th>
					<th>Lastname</th>
					<th>Email</th>
					<th>Password</th>
					<th>Status</th>
				</tr>
			</thead>
		      
		    <tbody>
				<% for(var i = 0; i < data.length; i++) { %>
				   		<tr>
				   			<td> <%= data[i].user_id %> </td>
				   			<td> <%= data[i].user_firstname %> </td>
				   			<td> <%= data[i].user_lastname %> </td>
				   			<td> <%= data[i].user_email %> </td>
				   			<td> <%= data[i].user_password %> </td>
				   			<td> <%= data[i].user_status %> </td>
				   		</tr>
				<% } %>
			</tbody>
		</table>
	</div>
</body>
</html>