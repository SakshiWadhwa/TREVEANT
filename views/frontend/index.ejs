	<button class="btn btn-lg back-to-top" id="back-to-top_hv" data-toggle="tooltip" data-placement="left">
		<span class="glyphicon glyphicon-chevron-up"></span>
	</button>

	<script>

		// $('#monitor').html($(window).width());

		// $(window).resize(function() {
		//     var viewportWidth = $(window).width();
		//     $('#monitor').html(viewportWidth);
		// });
		// jQuery for goto top button
		$(document).ready(function()
		{
	     $(window).scroll(function () {
	            if ($(this).scrollTop() > 50) {
	                $('#back-to-top_hv').fadeIn();
	            } else {
	                $('#back-to-top_hv').fadeOut();
	            }
	        });
	        // scroll body to 0px on click
	        $('#back-to-top_hv').click(function () {
	            $('#back-to-top_hv').tooltip('hide');
	            $('body,html').animate({
	                scrollTop: 0
	            }, 800);
	            return false;
	        });
	        
	        $('#back-to-top_hv').tooltip('show');

	});
</script>

