	<div class="col-md-12 container-fluid" id="testimonial_section">
		<div class="row heading text-center">
			<h1>REVIEWS<hr align="center" class="testimonial_highlight"></h1>
		</div>
		<div class="col-md-12 container">
			<div class="carousel slide carousel-fade" data-ride="carousel" id="testimonial_carousel">
				<div class="carousel-inner">
					<% 
						var cv= 0;
						for(var i=0; i < data_testimonial.length; i++) 
						{
							if(cv == 0)
							{
							   %>
								   <div class="item active">
							  <%}
							else
							{
								%>
									<div class="item">
								<%
							}
							%>
							<div class="col-md-4">
								<img src=" <%= data_testimonial[i].testimonial_img %>" class="img-responsive img-circle" id="circular_img">
							</div>
							<div class="col-md-8" id="testimonial_para">
								<p><i class="fas fa-quote-left" id="quote_icon"></i></p>
								<p>
									<%= data_testimonial[i].testimonial_review %>

								</p>
								<div class="testimonial_name">
									<h3><%= data_testimonial[i].testimonial_name %></h3>
									<h5><%= data_testimonial[i].testimonial_designation %></h5>
								</div>
							</div>
						</div>
						<% cv++; }
					 %>
							
				</div>
				<a class="left carousel-control" href="#testimonial_carousel" data-slide="prev">
				    <span class="fas fa-chevron-circle-left" id="left_arrow"></span>
				</a>
			    <a class="right carousel-control" href="#testimonial_carousel" data-slide="next">
				    <span class="fas fa-chevron-circle-right" id="right_arrow"></span>
				    <span class="sr-only">Next</span>
				</a>
			</div>
		</div>
	</div>

