<!--____________________________________________________________SERVICES_______________________________________________________-->
	<div class=" container col-md-12 text-center" id="service_section">
		<div class="row heading">
			<h1>SERVICES<hr align="center" class="sevice_highlight"></h1>
		</div>
		<div class="service_container">
			<% for(var i=0;i< data_category.length;i++) { %>
				<div class="col-md-4">
						<% if(typeof service_data =='undefined') { %>
							<a href="#" data-toggle="modal" data-target="#service_modal" id="service_panel_button_<%= data_category[i].category_id%>" data_value="<%= data_category[i].category_name%>" a_value="<%= data_category[i].category_id%>">
						<% } else {%>
							<a href="/wedding_service/<%= data_category[i].category_name%>/<%= data_category[i].category_id%>">
						<% } %>
								<span class="fa-stack fa-4x span_circle">
								    <i class="fa fa-circle fa-stack-2x icon-background"></i>
								    <i class="<%= data_category[i].category_icon%>"></i>
							    </span>
							</a>
					<h3><%= data_category[i].category_name%></h3>
					<hr class="underline">
					<p class="text">
						<%= data_category[i].category_description%>
					</p>

					<script>
						$("#service_panel_button_<%= data_category[i].category_id%>").on("click",function()
						{
							var t = $(this).attr("data_value");
							$("#cate_name").val(t);

							var a = $(this).attr("a_value");
							$("#cate_id").val(a);
						});
					</script>

				</div>
				
			<%}%>
		</div>	
	</div>


