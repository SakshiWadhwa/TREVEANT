<%- include header %> 
<style>
	.tab
	{
		padding-top: 120px;
	}
</style>

	<div class="table-responsive container tab">
		<h1 class="text-center"><%= data[0].testinomial_name %></h1>
		<table class="table table-striped table-hover table-bordered table-condensed">
			<thead>
				<tr>
					<th>Testinomial_Id</th>
					<th>Name</th>
					<th>Designation</th>
					<th>Review</th>
					<th>Image</th>
				</tr>
			</thead>
		      
		    <tbody>
				<% for(var i = 0; i < data.length; i++) { %>
				   		<tr>
				   			<td> <%= data[i].testimonial_id %> </td>
				   			<td> <%= data[i].testimonial_name %> </td>
				   			<td> <%= data[i].testimonial_designation %> </td>
				   			<td> <%= data[i].testimonial_review %> </td>
				   			<td> <%= data[i].testimonial_img %> </td>
				   		</tr>
				<% } %>
			</tbody>
		</table>
	</div>
</body>
</html>