<%- include header %> 
<style>
	.tab
	{
		padding-top: 120px;
	}
</style>

	<div class="table-responsive container tab">
		<h1 class="text-center"><%= data[0].membership_title %></h1>
		<table class="table table-striped table-hover table-bordered table-condensed">
			<thead>
				<tr>
					<th>Membership_Id</th>
					<th>Title</th>
					<th>Description</th>
					<th>Price</th>
					<th>Status</th>
				</tr>
			</thead>
		    <tbody>
				<% for(var i = 0; i < data.length; i++) { %>
				   		<tr>
				   			<td> <%= data[i].membership_id %> </td>
				   			<td> <%= data[i].membership_title %> </td>
				   			<td> <%= data[i].membership_description %> </td>
				   			<td> <%= data[i].membership_price %> </td>
				   			<td> <%= data[i].membership_status %> </td>
				   		</tr>
				<% } %>
			</tbody>
		</table>
	</div>
</body>
</html>