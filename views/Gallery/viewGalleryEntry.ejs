<%- include header %> 
<style>
	.tab
	{
		padding-top: 120px;
	}
</style>

	<div class="table-responsive container tab">
		<h1 class="text-center"><%= data[0].gallery_title %></h1>
		<table class="table table-striped table-hover table-bordered table-condensed">
			<thead>
				<tr>
					<th>Project_Id</th>
					<th>Title</th>
					<th>Image</th>
					<th>Description</th>
					<th>Status</th>
				</tr>
			</thead>
		    <tbody>
				<% for(var i = 0; i < data.length; i++) { %>
			   		<tr>
			   			<td> <%= data[i].project_id %></td>
			   			<td> <%= data[i].gallery_title %> </td>
			   			<td> <%= data[i].gallery_img %> </td>
			   			<td> <%= data[i].gallery_description %> </td>
			   			<td> <%= data[i].gallery_status %> </td>
			   		</tr>
				<% } %>
			</tbody>
		</table>
	</div>
</body>
</html>