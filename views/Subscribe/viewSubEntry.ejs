<%- include header %> 
<style>
	.tab
	{
		padding-top: 120px;
	}
</style>

<div class="table-responsive container tab">
	<h1 class="text-center"><%= data[0].subscribe_name %></h1>
		<table class="table table-striped table-hover table-bordered table-condensed">
			<thead>
				<tr>
					<th>Subscription_Id</th>
					<th>Name</th>
					<th>Email</th>
					<th>Status</th>
				</tr>
			</thead>
		      
		    <tbody>
				<% for(var i = 0; i < data.length; i++) { %>
			   		<tr>
			   			<td> <%= data[i].subscribe_id %> </td>
			   			<td> <%= data[i].subscribe_name %> </td>
			   			<td> <%= data[i].subscribe_email %> </td>
			   			<td> <%= data[i].subscribe_status %> </td>
			   		</tr>
				<% } %>
			</tbody>
		</table>
	</div>
</body>
</html>